@model List<MoneySearchEntities.MoneyClass>

<div id="body">
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>Type the amount of money to give you change:</h1>
                <h2></h2>
            </hgroup>
            <input type="number" /> 
            @Html.ActionLink("Convert","ConvertChange", "Home" , new { number=111 },  null  )
        </div>
    </section>
    <section class="content-wrapper main-content clear-fix">
        <h3>List of all searches</h3>
       @foreach (var element in Model)
       {
        <p>@Html.DisplayFor(m => element)</p>
       }
    </section>
</div>
